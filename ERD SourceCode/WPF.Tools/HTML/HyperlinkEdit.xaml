<src:WindowBase xmlns:CommonControls="clr-namespace:WPF.Tools.CommonControls"  xmlns:ModelViewer="clr-namespace:WPF.Tools.ModelViewer"  x:Class="WPF.Tools.HTML.HyperlinkEdit"
                xmlns:src="clr-namespace:WPF.Tools.BaseClasses"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                xmlns:local="clr-namespace:WPF.Tools.HTML"
                Title="Hyperlink"
                mc:Ignorable="d" 
                d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        
        <ModelViewer:ModelViewer Grid.Row="0" x:Name="uxViewer" />

        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
            <CommonControls:ActionButton Content="OK" Click="Ok_Cliked" />
            <CommonControls:ActionButton Content="Cancel" Click="Canceled_Click" />
        </StackPanel>
    </Grid>
</src:WindowBase>
