<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!--<SolidColorBrush Color="#414042" x:Key="SelectedBackground"/>

    <SolidColorBrush Color="DarkGray" x:Key="SelectedForeground"/>

    <SolidColorBrush Color="Gray" x:Key="DisabledForeground"/>-->

    <SolidColorBrush Color="Gray" x:Key="SelectedBackground"/>

    <SolidColorBrush Color="Black" x:Key="SelectedForeground"/>

    <SolidColorBrush Color="LightGray" x:Key="DisabledForeground"/>

    <LinearGradientBrush x:Key="TabGradientBrush" EndPoint="1,0" StartPoint="0,0">
        <LinearGradientBrush.RelativeTransform>
      <TransformGroup>
        <ScaleTransform CenterY="0.3" CenterX="0.3"/>
        <SkewTransform CenterY="0.3" CenterX="0.3"/>
        <RotateTransform Angle="90" CenterY="0.3" CenterX="0.3"/>
        <TranslateTransform/>
      </TransformGroup>
    </LinearGradientBrush.RelativeTransform>
        <GradientStop Color="#CFCFCF" Offset="0"></GradientStop>
        <GradientStop Color="#CFCFCF" Offset="0.268"/>
        <GradientStop Color="#CFCFCF" Offset="0.644"/>
        <GradientStop Color="#CFCFCF" Offset="0.879"/>
        <GradientStop Color="#CFCFCF" Offset="0.992"/>
    </LinearGradientBrush>

</ResourceDictionary>